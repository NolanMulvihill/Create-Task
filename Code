silloutees = 5 
#name, image url, silloutee, age, height, and weight
champs = (("Aatrox", aatrox_img, aatrox_all_black, 5000, 22.3, 4000), ("Ahri", ahri_img, ahri_all_black, 22, 5.5, 145), ("Akali", akali_img, akali_all_black, 19, 5.3, 110),
