def create_category():
    global category
    new = random.randint(1,3)
    #print(categories[new-1])
    category = new + 2
    print("Is " + champs[next_champ][0] + " " + categories[new - 1] + " than " +  champs[current_champ][0] + "?")
    
    
def create_next_champ():
    global next_champ
    global current_champ
    current_champ = next_champ
    
    while next_champ == current_champ:
        next_champ = random.randint(0, len(champs)-1)
    create_category()    
    display()
    
def display():
    global img
    global img2
    img = Image(champs[current_champ][1])
    img.set_position(0,0)
    img.set_size(get_width()/2,get_height())
    add(img)
    img2 = Image(champs[next_champ][1])
    img2.set_position(get_width()/2,0)
    img2.set_size(get_width()/2,get_height())
    add(img2)
def H_or_L(category):
    global guess
    if guess == 1:
        if champs[next_champ][category] > champs[current_champ][category]:
            print("Right")
            create_next_champ()
        else:
            print("Wrong")
    else:
        if guess == 0:
            if champs[next_champ][category] <= champs[current_champ][category]:
                print("Right")
                create_next_champ()
            else:
                print("Wrong")
                
def new_HorL():
    global current_champ
    global next_champ
    current_champ = random.randint(0, len(champs)-1)
    while next_champ == current_champ:
        next_champ = random.randint(0, len(champs)-1)
    
    create_category()    
    display()
    
    
def play():
    global game
    global img
    global img2
    global choices
    global texts
    global silloutee_img
    
    if game == 1:
        new_HorL()
    if game == 2:
        choices = []
        
            
        for txt in texts:
            
            remove(txt)
        guess_that_LoL_champ()
        
        
def guess(event):
    global game
    global guess
    global category
    
    
    if event.key == "ArrowLeft":
        play()
        
        
    if event.key == "ArrowRight":
        if game == 1:
            game = 2
            print("Game Changed")
        else:
            if game == 2:
                game = 1
                print("Game Changed")
        
    if event.key == "ArrowUp":
        if game == 1:
            guess = 1
            
            H_or_L(category)   
        if game == 2:
            if guess == 0:
                guess = 1
            else:
                if guess == 1:
                    guess = 2
                else:
                    if guess == 2:
                        guess = 3
                    else:
                        if guess == 3:
                            guess = 4
                        else:
                                if guess == 4:
                                    guess = 1
            print(guess)
            
            
    if event.key == "ArrowDown":
        if game == 1:
            guess = 0
            
                
            H_or_L(category)
        if game == 2:
            if guess == 0:
                print("Please enter in your guess beofre locking in your guess.")
            else:
                if guess == answer + 1:
                    print("Correct")
                else:
                    print("wrong")
                
def guess_that_LoL_champ():
    global guess
    global choices
    global champs
    global answer
    global texts
    global silloutees
    
    cover = Rectangle(get_width(), get_height())
    cover.set_position(0,0)
    cover.set_color(Color.white)
    add(cover)
    
    guess = 0
    champ_answer = random.randint(0,silloutees - 1)
    silloutee_img = Image(champs[champ_answer][2])
    silloutee_img.set_position(0,0)
    silloutee_img.set_size(get_width()/2,get_height())
    add(silloutee_img)
    
    answer = random.randint(0,3)
    
    mc = 0
    choices = []
    
    for i in range (4):
        if mc == answer:
            choices.append(champs[champ_answer][0])
        else:
            wrong_champ = random.randint(0, len(champs) - 1)
            if wrong_champ == champ_answer:
                wrong_champ = random.randint(0, len(champs) - 1)
            wrong_champ = check(wrong_champ)
            choices.append(champs[wrong_champ][0])
            
        mc = mc +1
    texts = []        
    
    seperation = get_height() / 5
    placement = seperation
    for a in choices:
        txt = Text(a)
        txt.set_position(get_width()/2, placement)
        placement = placement + seperation
        add(txt)
        texts.append(txt)

def check(next_choice):
    #checks list of mc guesses to see if any are the same
    
    global choices
    global champs
    
    for i in choices:
        
        if champs[next_choice][0] == i:
            next_choice = random.randint(0, len(champs) - 1)
            check(next_choice)
            
    return next_choice
        
  
add_key_down_handler(guess)


